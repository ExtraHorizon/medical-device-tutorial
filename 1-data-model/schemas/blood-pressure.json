{
  "$schema": "https://swagger.extrahorizon.com/cli/1.9.1/config-json-schemas/Schema.json",
  "name": "blood-pressure-measurement",
  "description": "Blood pressure measurement",
  "statuses": {
    "created": {}
  },
  "creationTransition": {
    "type": "manual",
    "toStatus": "created"
  },
  "properties": {
    "systolic": {
      "type": "number",
      "description": "Systolic pressure in mmHg"
    },
    "diastolic": {
      "type": "number",
      "description": "Diastolic pressure in mmHg"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the measurement was taken"
    }
  }
}
